{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "COVID-19 Vaccination Rate",
    "width": 800,
    "height": 600,
    "data": {
      "url": "https://raw.githubusercontent.com/WenTanZac0625/FIT3179/main/js/world_map.geom.json",
      "format": {
            "type": "topojson",
            "feature": "world.geom"
          }
    },
    "transform": [
      {
        "lookup": "properties.NAME",
        "from": {
          "data": {
            "url": "https://raw.githubusercontent.com/WenTanZac0625/FIT3179/main/data/Total_vaccination.csv"
          },
          "key": "COUNTRY",
          "fields": ["TOTAL_VACCINATIONS"]
        }
      },
      {"calculate": "datum.TOTAL_VACCINATIONS + 0.1", "as": "Total Vaccinations"}
    ],
    "projection": {
      "type": "equirectangular"
    },
    "mark": {
      "type": "geoshape"
      },
    "encoding": {
      "color": {
        "field": "Total Vaccinations",
        "type": "quantitative",
        "scale": {"type": "log"}
      },
      "tooltip": [
        {"field": "properties.NAME", "type": "nominal", "title": "Country"},
        {"field": "Total Vaccinations", "type": "quantitative"}
      ]
    }
  
  }
  